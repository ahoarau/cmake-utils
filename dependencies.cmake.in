include(CMakeFindDependencyMacro)

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@-component-${component}-link-libraries.cmake")

function(any_of list1 list2 result_var)
    foreach(item ${list1})
        if(${item} IN_LIST list2)
            set(${result_var} TRUE PARENT_SCOPE)
            return()
        endif()
    endforeach()
    set(${result_var} FALSE PARENT_SCOPE)
endfunction()

function(all_set list result_var)
    foreach(item ${list})
        if(NOT TARGET ${item})
            set(${result_var} FALSE PARENT_SCOPE)
            return()
        endif()
    endforeach()
    set(${result_var} TRUE PARENT_SCOPE)
endfunction()

